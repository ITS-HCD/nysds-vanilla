@section Scripts {
    <script>

 

                document.addEventListener("DOMContentLoaded", () => {
            const buttons = document.querySelectorAll('nys-button');
            const sections = document.querySelectorAll('#MPO, #Legislative, #Location');

            buttons.forEach(button => {
                button.addEventListener("nys-click", (event) => {
                    const buttonClass = event.target.className;
                    let targetSectionId = "";

                    if (buttonClass === "locationBtn") {
                        targetSectionId = "#Location";
                    } else if (buttonClass === "mpoBtn") {
                        targetSectionId = "#MPO";
                    } else if (buttonClass === "legislativeBtn") {
                        targetSectionId = "#Legislative";
                    }

                    Hide all sections by adding the "hidden" class
                    sections.forEach(section => section.setAttribute("class", "hidden"));

                    Show the target section by removing the "hidden" class
                    if (targetSectionId) {
                        document.querySelector(targetSectionId).removeAttribute("class");
                    }
                });
            });
        });



    </script>
}

